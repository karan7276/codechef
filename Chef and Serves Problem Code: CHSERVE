#include<bits/stdc++.h>
#include<string.h>
#define ll long long int
#define REPEAT int t;cin>>t;while(t--)
using namespace std;
int main()
{
   REPEAT{
      ll p1,p2,k,sum;
      cin>>p1>>p2>>k;
       sum=(p1+p2)/k;
      if(sum%2==0){
          cout<<"CHEF"<<endl;
      }else{
          cout<<"COOK"<<endl;
      }
   }
       
}
