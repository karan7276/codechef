#include<bits/stdc++.h>
#include<string.h>
#define ll long long int
#define REPEAT int t;cin>>t;while(t--)
using namespace std;
int main()
{
    REPEAT{
        ll n,i,c,p,k;
        cin>>c>>p>>k;
        ll ans=((c+p)/k);
        if(ans%2==0){
            cout<<"Chef"<<endl;
        }else{
            cout<<"Paja"<<endl;
        }
        
        
    }
    
    
    
}
